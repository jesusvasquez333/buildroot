BR2_ROOTFS_POST_IMAGE_SCRIPT="site/scripts/pack_img.sh"
BR2_ROOTFS_POST_SCRIPT_ARGS="${BR2_ARCH}"
BR2_LINUX_KERNEL=y
BR2_LINUX_KERNEL_CUSTOM_VERSION=y
