BR2_DL_DIR="$(TOPDIR)/../download"
BR2_HOST_DIR="$(TOPDIR)/../host/linux-${shell X=`uname -p`; case $$X in i?86 ) echo x86 ;; * ) echo $$X ;; esac}/$(BR2_ARCH)"
BR2_CCACHE=y
BR2_CCACHE_DIR="$(TOPDIR)/../host/buildroot-ccache"
BR2_ENABLE_DEBUG=y
BR2_OPTIMIZE_2=y
BR2_GLOBAL_PATCH_DIR="$(TOPDIR)/site/pkg-patches/"
BR2_TOOLCHAIN_BUILDROOT_GLIBC=y
BR2_TOOLCHAIN_BUILDROOT_CXX=y
BR2_TOOLCHAIN_BUILDROOT_FORTRAN=y
BR2_PACKAGE_MPFR=y
BR2_PACKAGE_HOST_GDB=y
